
    <%= form_with model: @shipping_address, url: shipping_address_path, method: :post do |f| %>
      <%= f.label :name%>
      <%= f.text_field :name, class: "space" %><br>
      <%= f.label :street1%>
      <%= f.text_field :street1, class: "space" %><br>
      <%= f.label :city%>
      <%= f.text_field :city, class: "space" %><br>
      <%= f.label :state%>
      <%= f.text_field :state, class: "space" %><br>
      <%= f.label :zip%>
      <%= f.text_field :zip, class: "space" %><br>
      <%= f.label :country%>
      <%= f.text_field :country, class: "space" %><br>
      <%= f.label :phone%>
      <%= f.text_field :phone, class: "space" %><br>
      <%= f.submit %>
    <% end %>
